{
  "name": "technova-2025",
  "version": "0.1.0",
  "private": true,
  "description": "Networking app frontend for underrepresented groups in male-dominated fields (gaming, tech conventions, sports).",
  "type": "module",
  "scripts": {
    "predev": "kill-port 5173 && kill-port 3000 || echo \"Ports cleared\"",
    "dev": "concurrently -k -n BACKEND,WEB -c blue,green \"npm:dev:backend\" \"npm:dev:web\"",
    "dev:backend": "npm --prefix backend run dev",
    "dev:web": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@google/generative-ai": "^0.24.1",
    "date-fns": "^2.30.0",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "socket.io-client": "^4.7.5",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.0",
    "jsdom": "^24.1.1",
    "kill-port": "^2.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.1.1"
  }
}
